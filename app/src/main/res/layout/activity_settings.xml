                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowEdit
                android:id="@+id/ilMediaAudioMicSource"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

        </LinearLayout>
    </ScrollView>

</LinearLayout>
