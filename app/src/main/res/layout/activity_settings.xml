            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowSw
                android:id="@+id/ilOnBoot"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowSw
                android:id="@+id/ilDozeWorkaround"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowSw
                android:id="@+id/ilDozeController"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowSw
                android:id="@+id/ilDozeAmRelax"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowSw
                android:id="@+id/ilWifiLock"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowEdit
                android:id="@+id/ilKaTcpMobile"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowEdit
                android:id="@+id/ilKaTcpWifi"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowEditIbIntRange
                android:id="@+id/ilMediaAudioPreAmpRx"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowEditIbIntRange
                android:id="@+id/ilMediaAudioPreAmpTx"
