                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowEditIntRange
                android:id="@+id/ilServerOutProxyPort"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowSw
                android:id="@+id/ilSwIce"
                android:layout_width="match_parent"
                android:layout_height="60dp"/>

            <include layout="@layout/compf_hbar"/>

            <com.meowsbox.vosp.widget.CompRowSw
